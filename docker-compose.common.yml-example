version: "3.8"
services:
  mysql:
    restart: unless-stopped
    image: mysql:8.3.0
    ports:
      - "3306:3306"
      - "33060:33060"
    volumes:
      - mysql:/var/lib/mysql
    environment:
      MYSQL_ROOT_PASSWORD: ${MYSQL_ROOT_PASSWORD}
      MYSQL_DATABASE: ${MYSQL_DATABASE}

volumes:
  mysql:
